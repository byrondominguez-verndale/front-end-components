{
  "name": "@verndale/front-end-components",
  "description": "Front end component library.",
  "license": "MIT",
  "version": "1.0.6",
  "main": "lib/components/main.js",
  "author": {
    "name": "Verndale",
    "email": "hello@verndale.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/verndale/front-end-components.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "build": "rimraf ./docs ./lib && yarn lint && yarn docs && npx babel src --out-dir lib && gulp copy",
    "docs": "documentation build src/components/**/*.js -f html -g true -o docs",
    "lint": "eslint --fix --color --debug --ignore-path .eslintignore ./src/components/**/*.js",
    "sass": "node-sass src/components/preview.scss -o public/css",
    "start": "yarn svg && yarn sass && gulp",
    "svg": "svgo public/images/svg-sprites -o public/images && svg-sprite-generate -d public/images/svg-sprites -o public/images/svgsheet.svg"
  },
  "dependencies": {
    "@verndale/core": "1.1.3",
    "babel-loader": "8.2.2",
    "merge-stream": "2.0.0",
    "sass-mq": "5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.14.7",
    "@babel/register": "7.14.5",
    "@frctl/fractal": "1.5.10",
    "babel-eslint": "10.1.0",
    "documentation": "13.2.5",
    "eslint": "7.31.0",
    "gulp": "4.0.2",
    "gulp-util": "3.0.8",
    "node-sass": "6.0.1",
    "rimraf": "3.0.2",
    "svg-sprite-generator": "0.0.7",
    "svgo": "2.3.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  },
  "keywords": [
    "components",
    "accessible",
    "front end",
    "library"
  ]
}
